obj-m += ko2iblnd.o
ko2iblnd-y := o2iblnd.o o2iblnd_cb.o o2iblnd_modparams.o

KDIR ?= /lib/modules/`uname -r`/build

ccflags-y := -I$(src)/../../include
ccflags-y += -I$(src)/../../../libcfs/include
ccflags-y += -include$(src)/../../../config.h

default:
	$(MAKE) -C $(KDIR) M=$$PWD

clean:
	$(MAKE) -C $(KDIR) M=$$PWD clean
