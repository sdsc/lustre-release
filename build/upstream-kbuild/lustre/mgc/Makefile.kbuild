obj-m += mgc.o
mgc-y := mgc_request.o lproc_mgc.o

KDIR ?= /lib/modules/`uname -r`/build

ccflags-y := -I$(src)/../include
ccflags-y += -I$(src)/../../libcfs/include
ccflags-y += -I$(src)/../../lnet/include
ccflags-y += -include$(src)/../../config.h

default:
	$(MAKE) -C $(KDIR) M=$$PWD

clean:
	$(MAKE) -C $(KDIR) M=$$PWD clean
